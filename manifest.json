{
  "manifest_version": 3,
  "name": "Pet-o-Meter",
  "version": "1.0",
  "description": "Virtual cat productivity tracker",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {"service_worker": "background.js"},
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "art/icon.png",
      "32": "art/icon.png",
      "48": "art/icon1.png"
    }
  },
  "icons": {
    "16": "art/icon.png",
    "48": "art/icon1.png",
    "128": "art/icon1.png"
  }
}
